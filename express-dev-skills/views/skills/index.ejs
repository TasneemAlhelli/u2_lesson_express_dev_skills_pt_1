<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
</head>
<body>
  <h1>List of Skills</h1>
  <ul>
    <% skills.forEach((skill) => { %>
      <li>
        <a href="/skills/<%= skill.id %>"><%= skill.name %></a>
        <a href="/skills/<%= skill.id %>/edit">Edit</a>
        <form action="/skills/<%= skill.id %>?__method=DELETE" method="POST">
          <button type="submit">X</button>
        </form>
      </li>
    <%})  %>
    <a href="/skills/new">Add Skill</a>
  </ul>
</body>
</html>